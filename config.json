{
    "description": "GVfs plugin for Docker",
    "documentation": "https://docs.docker.com/engine/extend/plugins/",
    "entrypoint": [
        "/usr/bin/docker-volume-gvfs",
        "daemon"
    ],
    "env": [
        {
            "name": "DEBUG",
            "settable": [
                "value"
            ],
            "value": "0"
        }
    ],
    "interface": {
        "socket": "gvfs.sock",
        "types": [
            "docker.volumedriver/1.0"
        ]
    },
    "linux": {
        "capabilities": [
            "CAP_SYS_ADMIN"
        ],
        "devices": [
            {
                "path": "/dev/fuse"
            }
        ]
    },
    "network": {
        "type": "host"
    },
    "mounts": [
        {"name": "known_hosts", "description": "Contain ssh fingerprint", "source": "/etc/docker-volumes/gvfs/known_hosts", "destination": "/root/.ssh/known_hosts", "type": "bind"}
    ],
    "propagatedmount": "/var/lib/docker-volumes/gvfs"
}
